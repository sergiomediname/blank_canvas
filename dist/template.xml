  <b:if cond="!data:view.isLayoutMode">
    <b:skin>
      <![CDATA[
      /*!
        * Name      : Blank Canvas
        * Version   : 1.0.0
        * License   : MIT
        *//*!
  
      Settings
      =====================
      # Show Top Header
      <Variable name="s.top_header" description="s.top_header" type="string" value="false"/>
      
      # Show Bottom Header
      <Variable name="s.bottom_header" description="s.bottom_header" type="string" value="false"/>
  
      Card
      =====================
      # Show snippet
      <Variable name="e.snippet" description="e.snippet" type="string" value="true"/>
      
      # Snippet length
      <Variable name="e.snippetLength" description="e.snippetLength" type="string" value="155"/>
      
      # Show meta
      <Variable name="e.meta" description="e.meta" type="string" value="true"/>
      
      # Cover ratio
      <Variable name="e.ratio" description="e.ratio" type="string" value="4:3"/>
  
      Customization
      =====================
      <Variable name="keycolor" description="Main Color" type="color" value="#6514d6" default="#6514d6"/>
      <Group description="Font">
        <Variable name="font.body" description="Content font" type="font" value="normal normal 16px var(--default-font)" default="normal normal 16px var(--default-font)"/>
      </Group>
      <Group description="Widths">
        <Variable name="width.container" description="Content width" type="length" value="1300px" max="2000px" min="100px" default="1300px"/>
        <Variable name="width.sidebar" description="Sidebar width" type="length" value="1300px" max="2000px" min="100px" default="1300px"/>
        <Variable name="width.post" description="Post width (stream)" type="length" value="1300px" max="2000px" min="100px" default="1300px"/>
      </Group>
  
      Hacks
      =====================
  
      All styles
      =====================*/
