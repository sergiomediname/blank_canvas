      </b:includable>
      <b:includable id='post:card'>
        <b:loop values='data:items ?: data:posts' var='post'>
          <article class='card'>
      
            <b:if cond='data:post.featuredImage'>
              <b:with
                value='["21:9", "16:9", "4:3", "1:1", "1:2", "2:3", "5:7"] contains data:skin.vars.e_ratio ? data:skin.vars.e_ratio : "4:3"'
                var='ratio'>
                <div expr:class='"card__thumb ratio" + " ratio--" + data:ratio'>
                  <b:class name='has-image' />
                  <a expr:href='data:post.url.canonical'>
                    <b:include name='@image'
                      data='{title: data:post.title.escaped, src: data:post.featuredImage, resize: 400, ratio: data:skin.vars.e_ratio, params: "rw"}' />
                  </a>
                </div>
              </b:with>
            </b:if>
      
            <div class='card__content'>
              <div class='card__wrap'>
                <h2 class='card__title'>
                  <a expr:href='data:post.url.canonical'>
                    <b:eval expr='data:post.title.escaped != "" ? data:post.title.escaped : data:messages.noTitle' />
                  </a>
                </h2>
      
                <b:if cond='[true, false] contains data:skin.vars.e_meta ? data:skin.vars.e_meta : true'>
                  <b:include name='post:meta' data='{class: "card__meta"}' />
                </b:if>
      
                <b:if
                  cond='(data:post.snippets.short snippet { linebreaks: false, links: false, ellipsis: false }) != "" and ([true, false] contains data:skin.vars.e_snippet ? data:skin.vars.e_snippet : true)'>
                  <p class="card__snippet">
                    <b:with
                      value='((data:skin.vars.e_snippetLength not in [true,false]) and (data:skin.vars.e_snippetLength + 0) == data:skin.vars.e_snippetLength) ? (data:skin.vars.e_snippetLength + 0) : 155'
                      var='snippet_length'>
                      <b:eval
                        expr='data:post.snippets.long snippet { length: data:snippet_length, links: false, linebreaks: false }' />
                    </b:with>
                  </p>
                </b:if>
              </div>
            </div>
          </article>
        </b:loop>
      </b:includable>
      <b:includable id='post:meta'>
        <div class='meta'>
          <b:class expr:name='data:class' />
      
          <a href="#" class='meta-item has-icon'>
            <img class='lazy img-change object-contain w-full h-full inset-0'
              expr:src='data:post.author.authorPhoto.image resizeImage 50' expr:alt='data:post.author.name'
              expr:title='data:post.author.name' width='25' height='25' />
          </a>
      
          <div class='meta-item has-icon'>
            <b:include data='{icon: "time-five"}' name='@icons' />
            <time expr:datetime="data:post.date.iso8601">
              <data:post.date />
            </time>
          </div>
      
          <div class="meta-item has-icon">
            <b:include data='{icon: "comment"}' name='@icons' />
            <data:post.numberOfComments />
          </div>
        </div>
      </b:includable>
      <b:includable id="post:view">
        <header class="post__header">
          <h1 class="post__title">
            <data:post.title.escaped />
          </h1>
          <b:if cond="data:view.isPost">
            <b:include name="post:meta" data='{class: "post__meta"}' />
          </b:if>
        </header>
        <section class="post__body typography">
          <data:post.body />
        </section>
        <footer class="post__comment">
          <b:include name="comment:main"
            data="{ depth: 2, texts: { delete: data:skin.vars.t_delete, reply: data:skin.vars.t_reply } }" />
        </footer>
      </b:includable>
      <b:includable id="comment:form">
        <div class="comments__editor">
          <b:if cond="data:post.allowNewComments">
            <div id="comments__form">
              <div class="comments__iframe">
                <iframe id="comments-editor" expr:src='data:post.commentFormIframeSrc'
                  height="66" />
              </div>
            </div>
            <b:if cond="data:this.messages.blogComment">
              <div class="comment-messages">
                <data:this.messages.blogComment />
              </div>
            </b:if>
            <textarea id="form-script">
              <data:post.cmtfpIframe />
            </textarea>
            <button id="form-restore" class="comments__restore-button">
              <b:eval expr="data:texts.restore ?: data:messages.postAComment" />
            </button>
            <script>
              /*<![CDATA[*/
              !function(){"use strict";var e=document.getElementById("comment-form"),r=document.getElementById("form-script"),t=document.getElementById("form-restore"),n=document.querySelectorAll("[data-parent-id]"),c="is-active",o="is-replying";function i(e){if(e){var r=e.value.replace(/<script.*?src='(.*?)'.*?><\/script>/,"$1");e.remove(),function(e){return new Promise((function(r,t){var n=document.createElement("script");n.src=e,n.onload=r,n.onerror=t,document.head.appendChild(n)}))}(r).then((function(){return BLOG_CMT_createIframe("https://www.blogger.com/rpc_relay.html")})).catch((function(e){return console.error(e)}))}}function a(e,r){if(e){var t=e.match(/<iframe[^>]*\s+src="([^"]*)"/i)[1];return{originalSrc:t,form:r?e.replace(t,r):e}}}!function(n){if(i(r),n){var m,l=e.innerHTML,s=a(l),d=s.originalSrc,u=s.form,f=document.createElement("div");f.id="reply-form",n.forEach((function(r){r.onclick=function(){var n=r.dataset.parentId,i=document.querySelector("#c".concat(n," .comments-replies")),s=document.getElementById("reply-form");if(m!==r){s?s.remove():(e.innerHTML="",t.classList.add(o)),m&&m.classList.remove(c);var u="".concat(d,"&parentID=").concat(n),p=a(l,u).form;r.classList.add(c),m=r,f.innerHTML=p,i.insertAdjacentElement("afterbegin",f)}}})),t&&(t.onclick=function(){if(m){var r=document.getElementById("reply-form");t.classList.remove(o),e.innerHTML=u,m.classList.remove(c),m=null,r.remove()}})}}(n)}();
      
              /*]]>*/
            </script>
            <b:else />
            <div class="comment-alert">
              <data:post.noNewCommentsText />
            </div>
          </b:if>
        </div>
      </b:includable>
      <b:includable id="comment:header">
        <div class="comment__header">
          <div class="comment__avatar">
            <b:include name="@avatar" data="{ src: data:comment.authorAvatarSrc, default: data:avatar }" />
          </div>
          <div class="comment__meta">
            <b:tag class="comment__author" expr:name='data:comment.authorUrl ? "a" : "span"'>
              <b:attr name="href" expr:value="data:comment.authorUrl" />
              <data:comment.author />
              <b:if cond="data:comment.extraIconClass">
                <span class="comment__badge">
                </span>
              </b:if>
            </b:tag>
            <span class="comment__time">
              <data:comment.timestamp />
            </span>
          </div>
        </div>
      </b:includable>
      <b:includable id="comment:body">
        <div class="comment__content">
          <b:class cond="data:comment.isDeleted" name="comment-deleted" />
          <data:comment.body />
        </div>
      </b:includable>
      <b:includable id="comment:footer">
        <div class="comment__footer">
          <b:if cond="data:hasDepth">
            <button class="comment__reply-button" expr:data-parent-id="data:comment.id">
              <b:eval expr="data:texts.reply ?: data:messages.postAComment" />
            </button>
          </b:if>
          <span expr:class="data:comment.adminClass">
            <a class="comment__delete-button" expr:href="data:comment.deleteUrl">
              <b:eval expr="data:texts.delete ?: data:messages.deleteComment" />
            </a>
          </span>
        </div>
      </b:includable>
      <b:includable id="comment:pagination">
        <div class="comments-paging">
          <b:if cond="data:post.hasOlderLinks">
            <a expr:href="data:post.oldestLinkUrl">
              <data:messages.oldest />
            </a>
            <a expr:href="data:post.olderLinkUrl">
              <data:messages.older />
            </a>
          </b:if>
          <data:post.commentRangeText />
          <b:if cond="data:post.hasNewerLinks">
            <a expr:href="data:post.newerLinkUrl">
              <data:messages.newer />
            </a>
            <a expr:href="data:post.newestLinkUrl">
              <data:messages.newest />
            </a>
          </b:if>
        </div>
      </b:includable>
      <b:includable id="comment:item" var="comment">
        <b:with value="(data:currentDepth ?: -1) + 1" var="currentDepth">
          <b:with value="(data:depth gt 0) and (data:currentDepth lt data:depth)" var="hasDepth">
            <div class="comment" expr:id='"c" + data:comment.id'>
              <div class="comment-block">
                <b:include name="comment:header" />
                <b:include name="comment:body" />
                <b:include name="comment:footer" />
                <b:if cond="data:hasDepth">
                  <b:with value="data:post.comments where (c => c.inReplyTo == data:comment.id)" var="replies">
                    <div class="comments-replies">
                      <b:if cond="data:replies.any">
                        <b:loop expr:reverse="data:reverse" values="data:replies" var="reply">
                          <b:include name="comment:item" data="reply" />
                        </b:loop>
                      </b:if>
                    </div>
                  </b:with>
                </b:if>
              </div>
            </div>
          </b:with>
        </b:with>
      </b:includable>
      <b:includable id="comment:build">
        <b:with value="{ min: (data:depth ?: 1), max: 5 }" var="depths">
          <b:with value="(data:depths.min >= data:depths.max) ? data:depths.max : data:depths.min" var="depth">
            <b:with value="data:post.comments where (c => (data:depth == 0 ? c.id : !c.inReplyTo))" var="comments">
              <div class="comments__content">
                <b:if cond="data:comments.any">
                  <b:loop expr:reverse="data:reverse" values="data:comments" var="comment">
                    <b:include name="comment:item" data="comment" />
                  </b:loop>
                </b:if>
              </div>
            </b:with>
          </b:with>
        </b:with>
      </b:includable>
      <b:includable id="comment:main">
        <b:if cond="not data:view.isPreview and data:post.allowComments">
          <div class="comments-title">
            <b:message name="messages.numberOfComments">
              <b:param expr:value="data:post.numberOfComments" name="numComments" />
            </b:message>
          </div>
          <div class="comments" id="comments">
            <b:loop reverse="data:order" values='["build","form"]' var="include">
              <b:include expr:name='"comment:" + data:include' />
            </b:loop>
            <b:include cond="data:post.commentPagingRequired" name="comment:pagination" />
          </div>
        </b:if>
      </b:includable>
      <b:includable id="widget:Header">
        <b:if cond='["top-header", "main-header", "bottom-header"] not contains data:widget.sectionId'>
          <data:messages.theresNothingHere />
          <b:else />
          <div class="header-logo">
            <b:if cond='data:image'>
              <a class="header-logo__image" expr:href='data:blog.homepageUrl'>
                <img expr:alt='data:title' expr:height='data:height' expr:src='data:image' expr:width='data:width' />
              </a>
            </b:if>
            <b:if cond='data:imagePlacement == "BEHIND"'>
              <div class="header-logo__title">
                <a expr:href='data:blog.homepageUrl'>
                  <data:title />
                </a>
              </div>
            </b:if>
            <b:if cond='data:description and data:imagePlacement != "REPLACE"'>
              <div class="header-logo__subtitle">
                <data:description />
              </div>
            </b:if>
          </div>
        </b:if>
      </b:includable>
      <b:includable id='widget:HTML'>
        <data:content />
      </b:includable>
      <b:includable id='widget:PageList'>
        <b:with value='data:widget.sectionId == "main-header"' var='is_menu'>
          <b:if cond='data:is_menu'>
            <input type="checkbox" id="show_menu" class="offcanvas-control" />
          </b:if>
          <div class='pages'>
            <b:if cond='data:is_menu'>
              <label class="offcanvas-switch" for="show_menu">
                <b:include data='{icon: "menu"}' name='@icons' />
              </label>
            </b:if>
            <b:class expr:cond='data:is_menu' name='pages--header' />
      
            <nav class='navbar'>
              <ul expr:class='data:is_menu ? "menu" : "nav"'>
                <b:if cond='data:is_menu'>
                  <b:include name='@menu' />
                  <b:else />
                  <b:include name='@navbar' />
                </b:if>
              </ul>
            </nav>
          </div>
        </b:with>
      </b:includable>
      <b:includable id='widget:Blog'>
        <b:with value='data:widgets.Blog1.allBylineItems' var='show'>
          <b:if cond='data:view.isSingleItem'>
            <article class="post">
              <b:with value='data:posts.first' var='post'>
                <b:include name='post:view' />
              </b:with>
            </article>
            <b:else />
            <b:if cond='data:posts.empty or data:view.isError'>
              <p>No items</p>
              <b:else />
              <div class='cards'>
                <b:include name='post:card' />
              </div>
            </b:if>
          </b:if>
        </b:with>
      </b:includable>
      <b:includable id='widget-title'>
        <b:include name='@attr' data='{["id"],["data-version"]}' />
        <b:if
          cond='["top-header", "main-header", "bottom-header"] not contains data:widget.sectionId and (data:defaultTitle or data:title.length != 0)'>
          <div class='widget-title'>
            <span class='widget-name'>
              <b:eval expr='data:defaultTitle ?: data:title' />
            </span>
          </div>
        </b:if>
      </b:includable>
    </b:defaultmarkup>
    <b:defaultmarkup type='All'>
      <b:includable id='main' var='this'>
        <b:include name='widget-title' />
        <b:include name='content' />
      </b:includable>
      <b:includable id='content'>
        <div class='widget-content'>
          <b:include expr:name='"widget:" + data:widget.type' />
        </div>
      </b:includable>
    </b:defaultmarkup>
    <b:defaultmarkup type='AdSense,Header,Blog'>
      <b:includable id='main' var='this'>
        <b:include name='@attr' data='{["id"],["data-version"]}' />
        <b:include expr:name='"widget:" + data:widget.type' />
      </b:includable>
      <b:includable id='content' />
    </b:defaultmarkup>
    <b:defaultmarkup type='BloggerButton,BlogList,Feed,ReportAbuse,Stats,Translate,Wikipedia'>
      <b:includable id='main' />
      <b:includable id='content' />
    </b:defaultmarkup>
  </b:defaultmarkups>
  <b:comment>/END CORE</b:comment>

</head>
<b:if cond='data:view.isLayoutMode'>
  <b:tag name='div' class='tools' cond='data:view.isLayoutMode'>
    <b:section id='tools' maxwidgets='1' showaddelement='false'>
      <b:widget id='Image50' cond='data:view.isLayoutMode' type='Image' title='Upload images' locked='true'
        visible='false' version='2' />
    </b:section>
  </b:tag>
</b:if>

<body>
  <div class="template">
    <header class="header">
      <b:if cond='["true", "false"] contains data:skin.vars.s_topHeader ? data:skin.vars.s_topHeader : false'>
        <div class="top-header">
          <div class="container">
            <div class="wrap">
              <b:section id='top-header'>
                <b:widget id='HTML222' type='HTML' title='Header top' locked='false' version='2' />
              </b:section>
            </div>
          </div>
        </div>
      </b:if>
    
      <div class="main-header">
        <div class="container">
          <div class="wrap">
            <b:section id='main-header'>
              <b:widget id='Header1' locked='false' title='Título (Encabezado)' type='Header' visible='true' />
              <b:widget id='PageList1' locked='false' title='Páginas' type='PageList' visible='true' />
            </b:section>
          </div>
        </div>
      </div>
    
      <b:if cond='["true", "false"] contains data:skin.vars.s_bottomHeader ? data:skin.vars.s_bottomHeader : false'>
        <div class="bottom-header">
          <div class="container">
            <div class="wrap">
              <b:section id='bottom-header'>
                <b:widget id='HTML111' type='HTML' title='Header top' locked='false' version='2' />
              </b:section>
            </div>
          </div>
        </div>
      </b:if>
    </header>
    <main class="main">
      <div class="container">
        <div class="wrap">
          <b:section id='content'>
            <b:widget id='Blog1' type='Blog' title='WIDGET_TITLE' version='2' />
          </b:section>
          <b:section id='sidebar' maxwidgets='10'
            cond="[true, false] contains data:skin.vars.l_sidebar ? data:skin.vars.l_sidebar : true">
            <b:widget id='HTML1' type='HTML' title='WIDGET_TITLE' locked='false' version='2' />
            <b:widget id='PageList2' type='PageList' title='WIDGET_TITLE' locked='false' version='2' />
          </b:section>
        </div>
      </div>
    </main>
    <footer class="footer">
      <data:blog.title />
    </footer>
  </div>
</body>

</html>
