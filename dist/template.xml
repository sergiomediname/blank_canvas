            <b:case value="menu" />
            <path d="M4 6h16v2H4V6Zm0 5h16v2H4v-2Zm0 5h16v2H4v-2Z"></path>
      
            <b:default />
            <p>no existe el icono</p>
          </b:switch>
        </svg>
      </b:includable>
